{
  "name": "hof-form-wizard",
  "version": "1.1.0",
  "description": "routing and request handling for a multi-step form processes",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit && npm run cover && npm run check-coverage && npm run snyk",
    "lint": "eslint .",
    "unit": "mocha test/. --recursive --require test/helpers",
    "cover": "istanbul cover _mocha -- -R dot test/. --recursive --require test/helpers",
    "check-coverage": "istanbul check-coverage --statement 85.53 --branch 81.58 --function 66.67 --line 86.21",
    "snyk": "snyk test",
    "snyk:dev": "snyk test --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOfficeForms/hof-form-wizard.git"
  },
  "author": "Leonard Martin <hello@lennym.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOfficeForms/hof-form-wizard/issues"
  },
  "homepage": "https://github.com/UKHomeOfficeForms/hof-form-wizard",
  "dependencies": {
    "csrf": "^3.0.2",
    "debug": "^2.1.2",
    "express": "^4.12.2",
    "hof-form-controller": "^1.0.0",
    "hof-model": "^2.0.0",
    "hogan.js": "^3.0.2",
    "i18n-lookup": "^0.1.0",
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-config-homeoffice": "^2.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.1.2",
    "reqres": "^1.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.14.3"
  }
}
